{
    "name": "missionreviewer",
    "version": "0.0.0",
    "private": true,
    "description": "Synixe Contractor's Mission Reviewer",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check '**/*.ts'",
        "lint": "eslint src-ts/**/*.ts",
        "package": "ncc build --source-map --license licenses.txt",
        "rust": "cargo build --release && cp target/release/missionreviewer dist/missionreviewer",
        "all": "npm run build && npm run format && npm run lint && npm run package && npm run rust"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/synixecontractors/missionreviewer.git"
    },
    "author": "Synixe Contractors",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.10.1",
        "@actions/github": "^6.0.1",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/node": "^20.11.20",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@vercel/ncc": "^0.38.1",
        "eslint": "^8.57.0",
        "js-yaml": "^4.1.0",
        "prettier": "3.2.5",
        "typescript": "^5.3.3"
    }
}
